@article{SIP-Ullmann,
  author    = {Julian R. Ullmann},
  title     = {An Algorithm for Subgraph Isomorphism},
  journal   = {J. {ACM}},
  volume    = {23},
  number    = {1},
  pages     = {31--42},
  year      = {1976},
  url       = {http://doi.acm.org/10.1145/321921.321925},
  doi       = {10.1145/321921.321925},
  timestamp = {Thu, 20 Nov 2003 12:28:25 +0100},
  biburl    = {http://dblp.uni-trier.de/rec/bib/journals/jacm/Ullmann76},
  bibsource = {dblp computer science bibliography, http://dblp.org},
  addendum  = {
  \citeauthor{SIP-Ullmann} presents a standard backtracking algorithm for the subgraph isomorphism problem.
  This is augmented with  a \emph{refinement procedure} to perform search tree pruning---the current state in the search corresponds to a subgraph isomorphism if and only if this refinement procedure makes no changes to the current state.
  Additionally, the design of this refinement procedure allows for use in a parallel context.
  It is observed that this search tree pruning procedure performs graph isomorphism more quickly (and on a larger instance) than the basic backtracking search, although performs worse than its contemporaries when used to evaluate maximum clique.
  }
}

