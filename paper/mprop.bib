@article{SIP-VF2,
  author    = {Luigi P. Cordella and
               Pasquale Foggia and
               Carlo Sansone and
               Mario Vento},
  title     = {A (Sub)Graph Isomorphism Algorithm for Matching Large Graphs},
  journal   = {{IEEE} Trans. Pattern Anal. Mach. Intell.},
  volume    = {26},
  number    = {10},
  pages     = {1367--1372},
  year      = {2004},
  url       = {http://dx.doi.org/10.1109/TPAMI.2004.75},
  doi       = {10.1109/TPAMI.2004.75},
  timestamp = {Sat, 12 Mar 2016 09:04:02 +0100},
  biburl    = {http://dblp.uni-trier.de/rec/bib/journals/pami/CordellaFSV04},
  bibsource = {dblp computer science bibliography, http://dblp.org},
  addendum = {
  \citeauthor{SIP-VF2} introduce the VF2 algorithm for solving the subgraph isomorphism problem on sizable instances, which is stated to be more space-efficient than their prior VF algorithm.
  In particular, VF is augmented with more memory-efficient data structures for building candidate matches and evaluating state feasibility.
  The algorithm is compared against Ullmann's algorithm and the Nauty algorithm in runtime and complexity---it is found to generally outperform its contemporaries in graph isomorphism, except on random graphs with low connectivity.
  }
}

@article{SIP-LAD,
  author    = {Christine Solnon},
  title     = {AllDifferent-based filtering for subgraph isomorphism},
  journal   = {Artif. Intell.},
  volume    = {174},
  number    = {12-13},
  pages     = {850--864},
  year      = {2010},
  url       = {http://dx.doi.org/10.1016/j.artint.2010.05.002},
  doi       = {10.1016/j.artint.2010.05.002},
  timestamp = {Thu, 09 Sep 2010 10:40:31 +0200},
  biburl    = {http://dblp.uni-trier.de/rec/bib/journals/ai/Solnon10},
  bibsource = {dblp computer science bibliography, http://dblp.org},
  addendum = {
  The work proposes a forward-checking algorithm (\emph{LAD}) for solving the subgraph isomorphism problem, based on a strong filtering procedure to prune branches of the search tree which admit no solution.
  This filtering technique makes use of \citeauthor{AllDiff}'s \emph{All-Different} general arc-consistent constraint propagator by posting a new AllDiff constraint on a node's neighbours after assignment.
  In comparison with VF2, ILF and Abscon, it generally outperforms its rivals on larger instances but is beaten on smaller instances due to the time complexity of filtering.
  }
}

@inproceedings{SIP-SND,
  author    = {Gilles Audemard and
               Christophe Lecoutre and
               Mouny Samy Modeliar and
               Gilles Goncalves and
               Daniel Cosmin Porumbel},
  title     = {Scoring-Based Neighborhood Dominance for the Subgraph Isomorphism
               Problem},
  booktitle = {Principles and Practice of Constraint Programming - 20th International
               Conference, {CP} 2014, Lyon, France, September 8-12, 2014. Proceedings},
  pages     = {125--141},
  year      = {2014},
  crossref  = {DBLP:conf/cp/2014},
  url       = {http://dx.doi.org/10.1007/978-3-319-10428-7_12},
  doi       = {10.1007/978-3-319-10428-7_12},
  timestamp = {Fri, 09 Oct 2015 10:03:30 +0200},
  biburl    = {http://dblp.uni-trier.de/rec/bib/conf/cp/AudemardLMGP14},
  bibsource = {dblp computer science bibliography, http://dblp.org},
  addendum = {
  A very strong filtering technique, \emph{SND}, designed for use in forward-checking solvers for the subgraph isomorphism problem is discussed, introduced as a generalisation of LAD filtering.
  \citeauthor{SIP-SND} establish a form of implied constraint: for arbitrary neighbourhood and scoring functions, after assignment a variable's neighbourhood must achieve a score no larger than the neighbourhood of its assigned value in the mapping.
  A specialisation of their framework is discussed wherein pattern vertices within a distance $k$ of one another must be mapped to target vertices at most a distance $k$ from one another---this weakened variant achieves a filtering level close to singleton arc-consistency, but is often slower in practice than LAD.
  }
}

