@article{Lens-Topology,
  author    = {Partha Niyogi and
               Stephen Smale and
               Shmuel Weinberger},
  title     = {A Topological View of Unsupervised Learning from Noisy Data},
  journal   = {{SIAM} J. Comput.},
  volume    = {40},
  number    = {3},
  pages     = {646--663},
  year      = {2011},
  url       = {http://dx.doi.org/10.1137/090762932},
